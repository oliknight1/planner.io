{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
			"quotes": ["error", "single"],
			"object-curly-spacing": ["warn","always"],
			"space-in-parens": ["warn", "always"],
			"array-bracket-spacing": ["warn","always"]
    }
}
